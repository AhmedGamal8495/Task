@using Task.Models
@model NwcInvoice

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>NwcInvoice</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NwcInvoicesNo" class="control-label">رقم الفاتوره</label>
                <input asp-for="NwcInvoicesNo" maxlength="10" min="10" class="form-control" />
                <span asp-validation-for="NwcInvoicesNo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NwcInvoicesDate" class="control-label">تاريخ الفاتوره</label>
                <input asp-for="NwcInvoicesDate" class="form-control" />
                <span asp-validation-for="NwcInvoicesDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NwcInvoicesFrom" class="control-label">من تاريخ</label>
                <input asp-for="NwcInvoicesFrom" class="form-control" />
                <span asp-validation-for="NwcInvoicesFrom" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NwcInvoicesTo" class="control-label">الى تاريخ</label>
                <input asp-for="NwcInvoicesTo" class="form-control" />
                <span asp-validation-for="NwcInvoicesTo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NwcInvoicesSubscriptionNo" class="control-label"> رقم الاشتراك</label>
                <select asp-for="NwcInvoicesSubscriptionNo" class ="form-control" asp-items="ViewBag.NwcInvoicesSubscriptionNo"></select>
            </div>
            <div class="form-group">
                <label asp-for="NwcInvoicesSubscriberNo" class="control-label">هوية المشترك</label>
                <select asp-for="NwcInvoicesSubscriberNo" class="form-control" asp-items="ViewBag.NwcInvoicesSubscriberNo"></select>
            </div>
 


            <div class="form-group">
                <label asp-for="NwcInvoicesPreviousConsumptionAmount" class="control-label">القراة السابقه</label>
                <input asp-for="NwcInvoicesPreviousConsumptionAmount" class="form-control" />
                <span asp-validation-for="NwcInvoicesPreviousConsumptionAmount" class="text-danger"></span>
            </div>
            <div class="form-group" >
                <label asp-for="NwcInvoicesCurrentConsumptionAmount" class="control-label">القراة الحاليه</label>
                <input asp-for="NwcInvoicesCurrentConsumptionAmount"  class="form-control" />
                <span asp-validation-for="NwcInvoicesCurrentConsumptionAmount" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NwcInvoicesAmountConsumption" class="control-label">كمية الاستهلاك</label>
                <input asp-for="NwcInvoicesAmountConsumption" readonly="readonly" class="form-control" />
                <span asp-validation-for="NwcInvoicesAmountConsumption" class="text-danger"></span>
            </div>


            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="NwcInvoicesIsThereSanitation" /> 
                    متوفر صرف صحي
                </label>
            </div>
            

            <div class="form-group">
                <label asp-for="NwcInvoicesServiceFee" class="control-label">رسوم الخدمه</label>
                <input asp-for="NwcInvoicesServiceFee" class="form-control" />
                <span asp-validation-for="NwcInvoicesServiceFee" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NwcInvoicesTaxRate" class="control-label">نسبة الضريبه</label>
                <input asp-for="NwcInvoicesTaxRate" class="form-control" />
                <span asp-validation-for="NwcInvoicesTaxRate" class="text-danger"></span>
            </div>
            
            <div class="form-group">
                <label asp-for="NwcInvoicesConsumptionValue" class="control-label">قيمة استهلاك المياه</label>
                <input asp-for="NwcInvoicesConsumptionValue" class="form-control" />
                <span asp-validation-for="NwcInvoicesConsumptionValue" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NwcInvoicesWastewaterConsumptionValue" class="control-label">قيمة استهلاك الصرف الصحى</label>
                <input asp-for="NwcInvoicesWastewaterConsumptionValue" class="form-control" />
                <span asp-validation-for="NwcInvoicesWastewaterConsumptionValue" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NwcInvoicesTotalInvoice" class="control-label">قيمة الفاتوره</label>
                <input asp-for="NwcInvoicesTotalInvoice" class="form-control" />
                <span asp-validation-for="NwcInvoicesTotalInvoice" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NwcInvoicesTaxValue" class="control-label">قيمة الضريبه</label>
                <input asp-for="NwcInvoicesTaxValue" class="form-control" />
                <span asp-validation-for="NwcInvoicesTaxValue" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NwcInvoicesTotalBill" class="control-label">اجمالى الفاتوره</label>
                <input asp-for="NwcInvoicesTotalBill" class="form-control" />
                <span asp-validation-for="NwcInvoicesTotalBill" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NwcInvoicesTotalReasons" class="control-label">ملاحظات</label>
                <input asp-for="NwcInvoicesTotalReasons" class="form-control" />
                <span asp-validation-for="NwcInvoicesTotalReasons" class="text-danger"></span>
            </div>





            <div class="form-group">
                <label asp-for="NwcInvoicesYear" class="control-label">السنه الماليه</label>
                <input asp-for="NwcInvoicesYear" class="form-control" />
                <span asp-validation-for="NwcInvoicesYear" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NwcInvoicesRrealEstateTypes" class="control-label">نوع العقار</label>
                <select asp-for="NwcInvoicesRrealEstateTypes" class="form-control" asp-items="ViewBag.NwcInvoicesRrealEstateTypes"></select>
            </div>



            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>

        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

