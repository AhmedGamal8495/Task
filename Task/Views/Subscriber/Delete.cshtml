@using Task.Models
@model NwcSubscriberFile

@{
    ViewData["Title"] = "Delete";
}


<h3 class="text-center" style="margintop:10px;">Are you sure you want to delete this?</h3>
<div>
    <h4>NwcSubscriberFile</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            اسم المشترك
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NwcSubscriberFileName)
        </dd>
        <dt class = "col-sm-2">
            المدينه
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NwcSubscriberFileCity)
        </dd>
        <dt class = "col-sm-2">
            المنطقه
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NwcSubscriberFileArea)
        </dd>
        <dt class = "col-sm-2">
            رقم الهاتف
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NwcSubscriberFileMobile)
        </dd>
        <dt class = "col-sm-2">
            ملاحظات
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NwcSubscriberFileReasons)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="NwcSubscriberFileId" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-primary">Back to List</a>
    </form>
</div>
