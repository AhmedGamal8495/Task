@using Task.Models
@model NwcSubscriptionFile

@{
    ViewData["Title"] = "Delete";
}



<h3 style="margin-top:10px;text-align:center;">Are you sure you want to delete this?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            ID
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NwcSubscriptionFileNo)
        </dd>
        <dt class = "col-sm-2">
            هل يوجد صرف
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NwcSubscriptionFileIsThereSanitation)
        </dd>
        <dt class = "col-sm-2">
            اخر قراءة للعداد
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NwcSubscriptionFileLastReadingMeter)
        </dd>
        <dt class = "col-sm-2">
            ملاحظات
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NwcSubscriptionFileReasons)
        </dd>
        <dt class = "col-sm-2">
            نوع العقار
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NwcSubscriptionFileRrealEstateTypesCodeNavigation.NwcRrealEstateTypesCode)
        </dd>
        <dt class = "col-sm-2">
            رقم هوية المشترك
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NwcSubscriptionFileSubscriberCodeNavigation.NwcSubscriberFileId)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="NwcSubscriptionFileNo" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
