@using Task.Models
@model IEnumerable<NwcRrealEstateType>

@{
    ViewData["Title"] = "Index";
}

<h1 style="margin-top:10px;"></h1>

<p>
    <a class="btn btn-primary" asp-action="Create">Create New</a>
</p>
<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th scope="col">
                ID
            </th>
            <th scope="col">
                اسم نوع العقار 
            </th>
            <th scope="col">
                ملاحظات حول العقار
            </th>
            <th>
                Actions
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td scope="row">
                    @Html.DisplayFor(modelItem => item.NwcRrealEstateTypesCode)
                </td>
                <td scope="row">
                    @Html.DisplayFor(modelItem => item.NwcRrealEstateTypesName)
                </td>
                <td scope="row">
                    @Html.DisplayFor(modelItem => item.NwcRrealEstateTypesReasons)
                </td>
                <td scope="row">
                    <a asp-action="Edit" class="btn btn-primary" asp-route-id="@item.NwcRrealEstateTypesCode">Edit</a> |
                    <a class="btn btn-danger" asp-action="Delete" asp-route-id="@item.NwcRrealEstateTypesCode">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
