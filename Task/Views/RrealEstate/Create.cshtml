            <div class="form-group">
                <label asp-for="NwcRrealEstateTypesReasons" class="control-label">ملاحظات</label>
                <input asp-for="NwcRrealEstateTypesReasons" class="form-control" />
                <span asp-validation-for="NwcRrealEstateTypesReasons" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
