@using Task.Models
@model NwcRrealEstateType
@{
    ViewData["Title"] = "Create";
}

<h1 style="margin-top:10px;">انشاء</h1>

<h4>نوع عقار جديد</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
