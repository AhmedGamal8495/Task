    <form asp-action="Delete">
        <input type="hidden" asp-for="NwcRrealEstateTypesCode" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a class="btn btn-primary" asp-action="Index">Back to List</a>
