@using Task.Models 
@model NwcRrealEstateType

@{
    ViewData["Title"] = "Delete";
}

<h3 class="text-center" style="margin-top:10px;">Are you sure you want to delete this?</h3>
<div>
    <hr/>
    <dl class="row">
        <dt class = "col-sm-2">
            اسم العقار
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NwcRrealEstateTypesName)
        </dd>
        <dt class = "col-sm-2">
            ملاحظات حول العقار
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NwcRrealEstateTypesReasons)
        </dd>
